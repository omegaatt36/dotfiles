[
  {
    "context": "Editor",
    "bindings": {
      "ctrl-f12": "editor::GoToImplementation",
      "ctrl-f11": "editor::GoToImplementationSplit",
      "shift-f12": "editor::GoToDeclaration",
      "ctrl-r": "outline::Toggle",
      "ctrl-pageup": "pane::ActivatePrevItem",
      "ctrl-pagedown": "pane::ActivateNextItem",
      "cmd-pageup": "pane::ActivatePrevItem",
      "cmd-pagedown": "pane::ActivateNextItem",
      "cmd-y": "editor::Redo"
    }
  },
  {
    "context": "ProjectPanel",
    "bindings": {
      "ctrl-.": "project_panel::NewFile",
      "ctrl-,": "project_panel::NewDirectory",
      "f2": "project_panel::Rename",
      // "ctrl-shift-c": "project_panel::CopyPath",
      "enter": "project_panel::Open"
    }
  }
]
